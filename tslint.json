{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [
                150
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "trailing-comma": [
            false,
            {
                "singleline": "never",
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                }
            }
        ],
        "object-literal-sort-keys": false,
        "triple-equals": false,
        "interface-name": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}